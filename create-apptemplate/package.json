  "repository": {
    "type": "git",
    "url": "git+https://github.com/abuhanna/app-template.git"
  },
  "bugs": {
    "url": "https://github.com/abuhanna/app-template/issues"
  },
  "homepage": "https://github.com/abuhanna/app-template#readme",
  "dependencies": {
    "@clack/prompts": "^0.9.1",
    "cross-spawn": "^7.0.6",
    "degit": "^2.8.4",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.6",
    "@types/degit": "^2.8.6",
    "@types/node": "^22.10.5",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3"
  }
