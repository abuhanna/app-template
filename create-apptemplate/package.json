{
  "name": "@abuhannaa/create-apptemplate",
  "version": "1.0.3",
  "description": "Create fullstack apps with .NET/Spring/NestJS + Vue (Vuetify/PrimeVue)",
  "author": "abuhanna",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "create-apptemplate": "./dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
